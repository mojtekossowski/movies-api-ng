{
  "name": "movies-api-ng",
  "version": "1.0.0",
  "description": "Movies rest API",
  "main": "server.js",
  "scripts": {
    "rollback": "knex migrate:latest --knexfile ./config/knexfile.js",
    "migrate": "knex migrate:latest --knexfile ./config/knexfile.js",
    "seed": "knex seed:run --knexfile ./config/knexfile.js",
    "pretest": "npm run-script migrate -- --env test && npm run-script seed -- --env test",
    "test": "NODE_ENV=test mocha --exit ./*/*.test.js",
    "postinstall": "npm run-script migrate"
  },
  "author": "mojtekossowski",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "knex": "^0.16.3",
    "morgan": "^1.9.1",
    "node-fetch": "^2.3.0",
    "pg": "^7.7.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "mocha": "^5.2.0"
  },
  "repository": {}
}
